<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}ArtGallery2022{% endblock %}</title>
    <link rel="icon" class="favi" href="{{ asset('img/favicon.png') }}">
    <link rel="stylesheet" href="{{ asset('css/bootstrap.min.css') }}">
    <link rel="stylesheet" href="{{ asset('css/main.css') }}">
    <link rel="stylesheet" href="{{ asset('css/icofont.min.css') }}">
    {% block stylesheets %}

    {% endblock %}

    {% block javascripts %}

    {% endblock %}
</head>
<body>
{% include 'partials/header.html.twig' %}
{% block body %}

{% endblock %}
{% include 'partials/footer.html.twig' %}
